 *
 * METHOD_NAME : WaitTime
 * METHOD_TYPE : -
 * DESCRIPTION :
 */
void CDioProcessStage2Sequence::WaitTime(DWORD dwMillisecond)
{
	DWORD dwStart;
	dwStart = GetTickCount();

	while (GetTickCount() - dwStart < dwMillisecond)
	{
		Sleep(10);
	}
}

